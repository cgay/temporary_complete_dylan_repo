// Driver loop.

define method do-action (action :: <accept>, state-stack :: <list>,
			 symbol-stack :: <list>, token :: <token>,
			 tokenizer :: <tokenizer>)
  unget-token(tokenizer, token);
  if (symbol-stack.size ~= 1)
    error("Symbol-stack didn't get reduced all the way?");
  end;
  symbol-stack.head;
end;

define method do-action (action :: <shift>, state-stack :: <list>,
			 symbol-stack :: <list>, token :: <token>,
			 tokenizer :: <tokenizer>)
  parse-loop(pair(action.state, state-stack),
	     pair(token, symbol-stack),
	     get-token(tokenizer),
	     tokenizer);
end;

define method do-action (action :: <reduce>, state-stack :: <list>,
			 symbol-stack :: <list>, token :: <token>,
			 tokenizer :: <tokenizer>)
  let (new-state-stack, new-symbol-stack)
    = *production-table*[action.production](state-stack, symbol-stack);
  parse-loop(new-state-stack, new-symbol-stack, token, tokenizer);
end;

define method parse-loop (state-stack :: <list>, symbol-stack :: <list>,
			  token :: <token>, tokenizer :: <tokenizer>)
  parse-loop-aux(state-stack, symbol-stack, token, tokenizer,
		 *action-table*[state-stack.head], #f);
end;

define method parse-loop-aux (state-stack :: <list>, symbol-stack :: <list>,
			      token :: <token>, tokenizer :: <tokenizer>,
			      action :: union(<action>, <false>),
			      prev :: union(<action>, <false>))
  if (action)
    if (instance?(token, action.on))
      if (prev)
	prev.next := action.next;
	let state = state-stack.head;
	action.next := *action-table*[state];
	*action-table*[state] := action;
      end;
      do-action(action, state-stack, symbol-stack, token, tokenizer);
    else
      parse-loop-aux(state-stack, symbol-stack, token, tokenizer,
		     action.next, action);
    end;
  else
    error("Parse error at or before %=", token);
    #f;
  end;
end;

define method parse (tokenizer :: <tokenizer>, start-state :: <integer>)
  parse-loop(list(start-state), #(), get-token(tokenizer), tokenizer);
end;

define method parse-program (tokenizer :: <tokenizer>) => ();
  parse(tokenizer, $program-start-state);
end;

define method parse-expression (tokenizer :: <tokenizer>)
    => res :: <expression>;
  parse(tokenizer, $expression-start-state);
end;

define method parse-variable (tokenizer :: <tokenizer>)
    => res :: <parameter>;
  parse(tokenizer, $variable-start-state);
end;

define method parse-body (tokenizer :: <tokenizer>)
    => res :: <simple-object-vector>;
  parse(tokenizer, $body-opt-start-state);
end;

define method parse-case-body (tokenizer :: <tokenizer>)
    => res :: <simple-object-vector>;
  parse(tokenizer, $case-body-start-state);
end;

define method parse-property-list (tokenizer :: <tokenizer>)
    => res :: <simple-object-vector>;
  parse(tokenizer, $property-list-sans-comma-start-state);
end;

define method parse-type (tokenizer :: <tokenizer>)
    => res :: <expression>;
  parse(tokenizer, $type-start-state);
end;
