pushd $OPEN_DYLAN_USER_BUILD/build

assemble

gcc -o build -L$OPEN_DYLAN_USER_INSTALL/lib -L$OPEN_DYLAN_RELEASE/lib -L/usr/lib -L/usr/X11R6/lib -L/usr/X11/lib -L/lib -L/usr/X11R6/lib/xrt/lib $OPEN_DYLAN_USER_INSTALL/lib/dylan-support.o *.o -ldylan -lcommon-dylan -lfunctional-dylan -lcollections -lio -lsystem -lsource-records -lfile-source-records -lrelease-info -linternal-release-info -lgeneric-arithmetic -lbig-integers -lbinary-manager -lbinary-builder -lcoff-manager -lcoff-builder -lbuild-system -lelf-linker -lccl-linker -llinker-support -lgnu-linker -Xlinker -rpath -Xlinker .

cp -fp build $OPEN_DYLAN_USER_INSTALL/lib/

