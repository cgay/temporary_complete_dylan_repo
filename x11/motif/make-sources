VendorS.dylan: $(MHDIR)/VendorS.h  $(PATTERN-DEP) $(COMSLOT) make-sources
	$(XTOD) -f $(COMSLOT)  -module Motif  -omit vendorShellWidgetClass \
	$(MHDIR)/VendorS.h $@
# vendorShellWidgetClass is omitted above because it has already
# been defined in "xt/Vendor.dylan".

VirtKeys.dylan: $(MHDIR)/VirtKeys.h  $(PATTERN-DEP) $(COMSLOT)
	$(XTOD) -f $(COMSLOT)  -module Motif  $(MHDIR)/VirtKeys.h $@

