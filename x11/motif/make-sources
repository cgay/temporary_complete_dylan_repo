
#  Dylan libraries for interfacing with Motif.
#
#  This file controls the generation of source files that are created
#  automatically by adapting information from the Motif C header files.

# Copyright (C) 1998 Functional Objects, Inc. All rights reserved.


MOTIF=Xm.dylan ArrowB.dylan ArrowBG.dylan AtomMgr.dylan BulletinB.dylan \
	CascadeB.dylan CascadeBG.dylan Command.dylan CutPaste.dylan \
	DialogS.dylan Display.dylan DragC.dylan DragIcon.dylan \
	DragOverS.dylan DrawingA.dylan DrawnB.dylan \
	DropSMgr.dylan DropTrans.dylan \
	FileSB.dylan Form.dylan Frame.dylan \
	Label.dylan LabelG.dylan List.dylan \
	MainW.dylan MenuShell.dylan MessageB.dylan \
	PanedW.dylan PushB.dylan PushBG.dylan \
	RepType.dylan RowColumn.dylan \
	Scale.dylan Screen.dylan ScrollBar.dylan ScrolledW.dylan \
	SelectioB.dylan SeparatoG.dylan Separator.dylan \
	Text.dylan TextF.dylan ToggleB.dylan ToggleBG.dylan \
	VendorS.dylan VirtKeys.dylan XmStrDefs.dylan library.dylan

all: $(MOTIF)

COMDIR=../xlib

include $(COMDIR)/sources.mak

library.dylan: library.src Xm.dylan \
	ArrowB.dylan ArrowBG.dylan AtomMgr.dylan \
	BulletinB.dylan \
	CascadeB.dylan CascadeBG.dylan Command.dylan CutPaste.dylan \
	DialogS.dylan Display.dylan DragC.dylan \
	DragIcon.dylan DragOverS.dylan DrawingA.dylan DrawnB.dylan \
	DropSMgr.dylan DropTrans.dylan \
	FileSB.dylan Form.dylan Frame.dylan \
	Label.dylan LabelG.dylan List.dylan \
	MainW.dylan MenuShell.dylan MessageB.dylan \
	PanedW.dylan PushB.dylan PushBG.dylan \
	RepType.dylan RowColumn.dylan \
	Scale.dylan Screen.dylan ScrollBar.dylan ScrolledW.dylan \
	SelectioB.dylan SeparatoG.dylan Separator.dylan \
	Text.dylan TextF.dylan ToggleB.dylan ToggleBG.dylan \
	VendorS.dylan VirtKeys.dylan XmStrDefs.dylan

Xm.dylan: $(MHDIR)/Xm.h  $(PATTERN-DEP) $(COMSLOT)
	$(XTOD) -f $(COMSLOT)  -module Motif  $(MHDIR)/Xm.h $@

ArrowB.dylan: $(MHDIR)/ArrowB.h  $(PATTERN-DEP) $(COMSLOT)
	$(XTOD) -f $(COMSLOT)  -module Motif  $(MHDIR)/ArrowB.h $@

ArrowBG.dylan: $(MHDIR)/ArrowBG.h  $(PATTERN-DEP) $(COMSLOT)
	$(XTOD) -f $(COMSLOT)  -module Motif  $(MHDIR)/ArrowBG.h $@

