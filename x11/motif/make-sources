	$(XTOD) -f $(COMSLOT)  -module Motif  $(MHDIR)/XmStrDefs.h $@

# remove all generated files
clean:
	rm -f $(MOTIF) *.bak *.exp

# remove internal temporary files
tidy:
	rm -f *.bak *.exp

DTAGS=/u/dylan/tools/gnuemacs/dtags
TAGS: $(MOTIF) extra.dylan
	$(DTAGS) extra.dylan $(MOTIF) ../xt/*.dylan ../xlib/*.dylan

tags: TAGS
