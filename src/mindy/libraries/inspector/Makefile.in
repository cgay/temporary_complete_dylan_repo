##  This is the makefile for the "demo" directory
##

SHELL	=	/bin/sh

##
## begin autoconf'ed source
##

prefix	=		@prefix@
exec_prefix	=	@exec_prefix@
srcdir	=		@srcdir@

SRCDIR	=		${srcdir}
BINDIR	=		${exec_prefix}/bin
LIBDIR	=		${exec_prefix}/lib/mindy

CC	=		@CC@
CCOPTS	=		@CCOPTS@
CFLAGS	=		$(CCOPTS) -I${SRCDIR} @DEFS@ -DBINDIR=\"${BINDIR}\" -DLIBDIR=\"${LIBDIR}\"
YACC	=		@YACC@
YFLAGS	=		-d
LEX	=		@LEX@
LFLAGS	=		@LFLAGS@
RANLIB	=		@RANLIB@
INSTALL	=		@top_srcdir@/install.sh
INSTALL_PROGRAM	=	@INSTALL_PROGRAM@
INSTALL_DATA	=	@INSTALL_DATA@

LIBOBJS =		@LIBOBJS@
LIBS	=		@LIBS@

##
## end of configure written portion
##

MC	=	mindycomp

MINDYFLAGS = -linspector

OBJS = temp.dbc library.dbc inspector.dbc

main: ${OBJS}

inspector.dbc: ${SRCDIR}/temp.dbc ${SRCDIR}/library.dbc
	cat ${SRCDIR}/library.dbc ${SRCDIR}/temp.dbc > ${SRCDIR}/inspector.dbc
##	rm ${SRCDIR}/test.dbc
##	mv ${SRCDIR}/test.dbc.temp ${SRCDIR}/test.dbc

library.dbc: ${SRCDIR}/library.dylan
	${MC} ${MINDYFLAGS} -o library.dbc ${SRCDIR}/library.dylan

temp.dbc: ${SRCDIR}/inspector.dylan
	${MC} ${MINDYFLAGS} -o temp.dbc ${SRCDIR}/inspector.dylan

clean:
	rm -f ${SRCDIR}/library.dbc ${SRCDIR}/temp.dbc *~ \#* core

realclean:
	rm -f ${OBJS} Makefile *~ \#* core

install:





