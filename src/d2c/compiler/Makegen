
do compile_subdirs(
    'base',
    'front',
    'parser',
    'cback',
    'main'
);

$stage2 || print <<'EOF';
.PHONY: d2c-compile

d2c-compile: compile
	$(MAKE) -C base d2c-compile
	$(MAKE) -C front d2c-compile
	$(MAKE) -C parser d2c-compile
	$(MAKE) -C cback d2c-compile
	$(MAKE) -C main d2c-compile

d2c-install:
	$(MAKE) -C main d2c-install

EOF
