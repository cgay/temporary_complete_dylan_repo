SHELL	=	/bin/sh

##
## begin autoconf'ed source
##

prefix	=		@prefix@
exec_prefix	=	@exec_prefix@
srcdir	=		@srcdir@

SRCDIR	=		${srcdir}
BINDIR	=		@bindir@
LIBDIR	=		@libdir@/dylan

CC	=		@CC@
CCOPTS	=		@CCOPTS@
CFLAGS	=		$(CCOPTS) -I${SRCDIR} @DEFS@ -DBINDIR=\"${BINDIR}\" -DLIBDIR=\"${LIBDIR}\"
YACC	=		@YACC@
YFLAGS	=		-d
LEX	=		@LEX@
LFLAGS	=		@LFLAGS@
RANLIB	=		@RANLIB@
INSTALL	=		@INSTALL@
INSTALL_PROGRAM	=	@INSTALL_PROGRAM@
INSTALL_DATA	=	@INSTALL_DATA@

LIBOBJS =		@LIBOBJS@
LIBS	=		@LIBS@

MC	=		@MC@

##
## end of configure written portion
##


MINDYFLAGS = -ldiff

OBJS = diff.dbc

main: ${OBJS}

diff.dbc: ${SRCDIR}/diff.dylan
	${MC} ${MINDYFLAGS} -o diff.dbc ${SRCDIR}/diff.dylan

clean:
	rm -f ${OBJS} *~ \#* core

realclean:
	rm -f ${OBJS} Makefile *~ \#* core

install:
