// Where to find various important files.  (So far the only important
// file is targets.descr)

define constant $default-targets-dot-descr
#if (compiled-for-x86-win32)
  #if (C-compiler-is-gcc)
    = "/gwydion/etc/targets.descr"; // the gcc runtime doesn't do drive letters
  #else
    = "c:/gwydion/etc/targets.descr";
  #endif
#elseif (compiled-for-hppa-hpux)
  = "/afs/cs/project/gwydion/compiler/etc/targets.descr";
#else
  = #f;  // no default
#endif
